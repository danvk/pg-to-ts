    "postgres",
    "schema",
    "typescript",
    "sql"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=8.15.1"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
