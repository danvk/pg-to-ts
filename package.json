{
  "name": "pg-to-ts",
  "version": "4.0.0",
  "description": "Generate TypeScript-friendly interfaces and constants from (postgres) SQL database schema",
  "keywords": [
    "postgres",
    "schema",
    "typescript",
    "sql"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=8.15.1"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "build": "tsc",
    "format": "prettier --write .",
    "test": "jest --runInBand",
    "clean": "rm -rf dist",
    "coverage": "jest --coverage --runInBand",
    "coverage:ci": "jest --coverage  --runInBand && nyc report -r text -r lcov"
  },
  "bin": {
    "pg-to-ts": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danvk/pg-to-ts.git"
  },
  "bugs": {
    "url": "https://github.com/danvk/pg-to-ts/issues"
  },
  "author": "Dan Vanderkam <danvdk@gmail.com>",
  "contributors": [
    "Mengxuan Xia <xiamx2004@gmail.com>",
    "Arnaud Benhamdine <arnaud.benhamdine@gmail.com>",
    "zigomir <zigomir@gmail.com>",
    "Mark Crisp <macr1324@gmail.com>",
    "George MacKerron <george@psyt.co.uk>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/diff": "^3.2.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.149",
    "@types/mz": "^0.0.31",
    "@types/node": "^13.7.6",
    "@types/power-assert": "^1.5.3",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "diff": "^3.2.0",
    "eslint": "^8.9.0",
    "jest": "^27.5.1",
    "mz": "^2.6.0",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "ts-node": "^3.0.4",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "pg-promise": "^10.11.1",
    "typescript-formatter": "^7.0.1",
    "yargs": "^17.3.1"
  },
  "peerDependencies": {
    "typescript": "^4.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
