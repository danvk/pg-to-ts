    "George MacKerron <george@psyt.co.uk>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "14",
    "@types/pluralize": "^0.0.29",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.9.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
